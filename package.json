{
  "name": "arriel-vanto-landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "bun predeploy && gh-pages -d dist",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:net": "docker network prune -f && docker network create app-network",
    "docker:build": "docker compose -f compose.yaml build",
    "docker:up": "docker compose -f compose.yaml up",
    "docker:down": "docker compose -f compose.yaml down",
    "docker": "npm run docker:net && npm run docker:build && npm run docker:up"
  },
